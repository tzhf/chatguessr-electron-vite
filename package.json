    "@types/better-sqlite3": "^7.6.7",
    "@types/google.maps": "^3.54.7",
    "@types/tmi.js": "^1.8.5",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
