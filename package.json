    "datatables.net-buttons": "^2.4.2",
    "datatables.net-plugins": "^1.13.6",
    "datatables.net-scroller": "^2.3.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.1.0",
    "format-duration": "^3.0.2",
    "jquery": "^3.7.1",
